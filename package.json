{
  "name": "video-calls",
  "version": "1.0.0",
  "private": true,
  "description": "Video Calls - video calls application.",
  "scripts": {
    "start": "tsc --esModuleInterop --skipLibCheck ./webpack.config.ts && webpack serve --env development --mode development --config webpack.config.js",
    "build": "tsc --esModuleInterop --skipLibCheck ./webpack.config.ts && webpack --env production --mode production --config webpack.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook",
    "deploy": "yarn run build && npx gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOINITID/video-calls.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "react",
    "redux-toolkit",
    "storybook",
    "emotion",
    "webpack"
  ],
  "author": "Igor Orishchenko <ioinitid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IOINITID/video-calls/issues"
  },
  "homepage": "https://github.com/IOINITID/video-calls#readme",
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hooks-kit": "^1.3.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-toastify": "^8.1.0",
    "simple-peer": "^9.11.0",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@svgr/webpack": "^6.2.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.22",
    "@types/simple-peer": "^9.11.4",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "esbuild-loader": "^2.18.0",
    "eslint": "^7.30.0",
