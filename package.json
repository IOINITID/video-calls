    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.7",
    "lint-staged": "^11.0.1",
    "prettier": "^2.5.1",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "^13.13.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
